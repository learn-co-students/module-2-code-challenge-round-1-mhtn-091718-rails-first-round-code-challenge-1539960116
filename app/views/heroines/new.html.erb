<h1>Enter Superheroine Here:</h1>

<% if flash[:error] %>
  <% flash[:error].each do |error| %>
    <h2><%= error %></h2>
  <% end %>
<% end %>

<%= form_for @heroine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %>
  <%= f.collection_select(:power_id, @powers, :id, :name) %>
  <%= f.submit %>
<% end %>
